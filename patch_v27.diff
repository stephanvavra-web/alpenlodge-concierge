diff -ruN alpenlodge/assets/js/al-booking-api.js alpenlodge/assets/js/al-booking-api.js
--- alpenlodge/assets/js/al-booking-api.js	1970-01-01 00:00:00.000000000 +0000
+++ alpenlodge/assets/js/al-booking-api.js	2026-01-13 21:57:50.107099495 +0000
@@ -0,0 +1,552 @@
+/* Alpenlodge – API Booking UI
+   - Uses the Concierge backend:
+     - POST /concierge/availability
+     - POST /concierge/book
+     - GET  /api/units   (optional, falls back to /api/units.json on the website)
+*/
+
+(() => {
+  const $ = (sel, root = document) => root.querySelector(sel);
+
+  const state = {
+    apiBase: "",
+    endpoints: { units: "", availability: "", book: "" },
+    unitsById: {},
+    unitsLoaded: false,
+    activeCategory: "All",
+    lastOffers: [],
+    selectedOffer: null,
+    preselectApartmentId: null,
+  };
+
+  function slugifyDE(name) {
+    const s0 = String(name || "").trim().toLowerCase();
+    // German-specific folding
+    const s1 = s0
+      .replace(/ä/g, "ae")
+      .replace(/ö/g, "oe")
+      .replace(/ü/g, "ue")
+      .replace(/ß/g, "ss");
+    // Remove remaining diacritics
+    const s2 = s1.normalize("NFD").replace(/\p{Diacritic}+/gu, "");
+    return s2.replace(/[^a-z0-9]+/g, "-").replace(/^-+|-+$/g, "");
+  }
+
+  function getApiBase() {
+    const api = window.AL_CONCIERGE_API || "https://alpenlodge-concierge.onrender.com/api/concierge";
+    try {
+      return new URL(api, window.location.origin).origin;
+    } catch (e) {
+      return "https://alpenlodge-concierge.onrender.com";
+    }
+  }
+
+  async function fetchJson(url, options) {
+    const res = await fetch(url, options);
+    const ct = (res.headers.get("content-type") || "").toLowerCase();
+    let data = null;
+    try {
+      if (ct.includes("application/json")) data = await res.json();
+      else data = await res.text();
+    } catch (e) {
+      data = null;
+    }
+
+    if (!res.ok) {
+      const err = new Error("Request failed");
+      err.status = res.status;
+      err.data = data;
+      throw err;
+    }
+    return data;
+  }
+
+  function setStatus(msg, kind = "") {
+    const box = $("#status");
+    if (!box) return;
+    box.textContent = msg || "";
+    box.dataset.kind = kind || "";
+  }
+  function setBookStatus(msg, kind = "") {
+    const box = $("#bookStatus");
+    if (!box) return;
+    box.textContent = msg || "";
+    box.dataset.kind = kind || "";
+  }
+
+  function fmtMoney(amount, currency) {
+    const n = Number(amount);
+    if (!Number.isFinite(n)) return "";
+    const cur = String(currency || "").trim();
+    try {
+      if (cur && cur.length === 3) {
+        return new Intl.NumberFormat("de-AT", { style: "currency", currency: cur }).format(n);
+      }
+    } catch {}
+    const sym = (cur === "€" || cur === "EUR") ? "€" : cur;
+    return sym ? `${n.toFixed(0)} ${sym}` : `${n.toFixed(0)}`;
+  }
+
+  function dayDiff(aIso, bIso) {
+    try {
+      const a = new Date(aIso + "T00:00:00Z");
+      const b = new Date(bIso + "T00:00:00Z");
+      const d = Math.round((b.getTime() - a.getTime()) / 86400000);
+      return Number.isFinite(d) ? d : null;
+    } catch {
+      return null;
+    }
+  }
+
+  const CATEGORIES = [
+    { key: "All", label: "Alle" },
+    { key: "Apartment", label: "Apartments" },
+    { key: "Suite", label: "Suiten" },
+    { key: "Premium", label: "Premium" },
+  ];
+
+  function renderCategoryChips() {
+    const row = $("#categoryChips");
+    if (!row) return;
+    row.innerHTML = "";
+    CATEGORIES.forEach((c) => {
+      const btn = document.createElement("button");
+      btn.type = "button";
+      btn.className = "chip" + (state.activeCategory === c.key ? " active" : "");
+      btn.textContent = c.label;
+      btn.addEventListener("click", () => {
+        state.activeCategory = c.key;
+        renderCategoryChips();
+        renderResults();
+      });
+      row.appendChild(btn);
+    });
+  }
+
+  function indexUnits(units) {
+    state.unitsById = {};
+    (units || []).forEach((u) => {
+      const idRaw = u?.smoobu_id ?? u?.apartmentId ?? u?.id;
+      const id = Number(idRaw);
+      if (!Number.isFinite(id)) return;
+
+      const name = u?.name || u?.internalName || `Apartment ${id}`;
+      const category = u?.category || "Other";
+      const detailsUrl = u?.details_url || (name ? `/pages/units/${slugifyDE(name)}.html` : null);
+
+      state.unitsById[id] = {
+        ...u,
+        id,
+        name,
+        category,
+        details_url: detailsUrl,
+        book_url: u?.book_url || `/buchen/?apartmentId=${id}`,
+      };
+    });
+    state.unitsLoaded = true;
+  }
+
+  async function loadUnits() {
+    // 1) Try backend (recommended)
+    try {
+      const data = await fetchJson(state.endpoints.units, { method: "GET" });
+      if (data && Array.isArray(data.units)) return data.units;
+    } catch {}
+
+    // 2) Fallback: static file on the website
+    try {
+      const data = await fetchJson("/api/units.json", { method: "GET" });
+      if (data && Array.isArray(data.units)) return data.units;
+      if (data && data.units && Array.isArray(data.units.units)) return data.units.units;
+    } catch {}
+
+    return [];
+  }
+
+  function setUnitPillIfAny() {
+    const pill = $("#unitPill");
+    if (!pill) return;
+
+    if (!state.preselectApartmentId) return;
+    const u = state.unitsById[state.preselectApartmentId];
+    pill.style.display = "inline-flex";
+    pill.textContent = u ? `Vorauswahl: ${u.name}` : `Vorauswahl: ${state.preselectApartmentId}`;
+  }
+
+  function clearResults() {
+    state.lastOffers = [];
+    state.selectedOffer = null;
+    const results = $("#results");
+    if (results) results.innerHTML = "";
+    const details = $("#bookingDetails");
+    if (details) details.style.display = "none";
+    setBookStatus("");
+  }
+
+  function resetForm() {
+    clearResults();
+    setStatus("");
+    // Keep dates/guests as-is (user friendly)
+  }
+
+  function filterOffers(offers) {
+    if (state.activeCategory === "All") return offers;
+    return offers.filter((o) => (o.unit?.category || o.category || "") === state.activeCategory);
+  }
+
+  function groupByCategory(offers) {
+    const groups = {};
+    offers.forEach((o) => {
+      const cat = o.unit?.category || o.category || "Other";
+      if (!groups[cat]) groups[cat] = [];
+      groups[cat].push(o);
+    });
+
+    Object.values(groups).forEach((arr) => {
+      arr.sort((a, b) => {
+        const pa = Number(a.price);
+        const pb = Number(b.price);
+        const aa = Number.isFinite(pa) ? pa : Infinity;
+        const bb = Number.isFinite(pb) ? pb : Infinity;
+        return aa - bb;
+      });
+    });
+
+    return groups;
+  }
+
+  function renderOfferCard(offer) {
+    const card = document.createElement("div");
+    card.className = "offer";
+
+    const title = document.createElement("div");
+    title.className = "offer-title";
+    title.textContent = offer.unit?.name || offer.name || `Apartment ${offer.apartmentId}`;
+    card.appendChild(title);
+
+    const meta = document.createElement("div");
+    meta.className = "offer-meta";
+    const bits = [];
+    if (offer.nights) bits.push(`${offer.nights} Nächte`);
+    if (offer.guests) bits.push(`${offer.guests} Gäste`);
+    if (offer.unit?.category) bits.push(offer.unit.category);
+    meta.textContent = bits.join(" · ");
+    card.appendChild(meta);
+
+    const price = document.createElement("div");
+    price.className = "offer-price";
+    price.textContent = fmtMoney(offer.price, offer.currency) || "Preis auf Anfrage";
+    card.appendChild(price);
+
+    const actions = document.createElement("div");
+    actions.className = "offer-actions";
+
+    if (offer.unit?.details_url) {
+      const a = document.createElement("a");
+      a.className = "btn link";
+      a.href = offer.unit.details_url;
+      a.textContent = "Details";
+      actions.appendChild(a);
+    }
+
+    const btn = document.createElement("button");
+    btn.type = "button";
+    btn.className = "btn primary";
+    btn.textContent = "Buchen";
+    btn.addEventListener("click", () => openBooking(offer));
+    actions.appendChild(btn);
+
+    card.appendChild(actions);
+    return card;
+  }
+
+  function renderResults() {
+    const box = $("#results");
+    if (!box) return;
+    box.innerHTML = "";
+
+    const offersFiltered = filterOffers(state.lastOffers || []);
+    if (!offersFiltered.length) {
+      const empty = document.createElement("div");
+      empty.className = "smallnote";
+      empty.textContent = state.lastOffers.length
+        ? "Keine passenden Ergebnisse für diese Kategorie."
+        : "Noch keine Ergebnisse – bitte Daten wählen und auf „Verfügbarkeit prüfen“ klicken.";
+      box.appendChild(empty);
+      return;
+    }
+
+    const groups = groupByCategory(offersFiltered);
+    const order = ["Premium", "Suite", "Apartment"];
+    const cats = Object.keys(groups).sort((a, b) => {
+      const ia = order.indexOf(a);
+      const ib = order.indexOf(b);
+      if (ia === -1 && ib === -1) return a.localeCompare(b);
+      if (ia === -1) return 1;
+      if (ib === -1) return -1;
+      return ia - ib;
+    });
+
+    cats.forEach((cat) => {
+      const wrap = document.createElement("div");
+      wrap.className = "result-group";
+
+      const h = document.createElement("h2");
+      h.textContent = cat === "Suite" ? "Suiten" : cat === "Apartment" ? "Apartments" : cat;
+      wrap.appendChild(h);
+
+      const list = document.createElement("div");
+      list.className = "offer-list";
+
+      groups[cat].forEach((o) => list.appendChild(renderOfferCard(o)));
+
+      wrap.appendChild(list);
+      box.appendChild(wrap);
+    });
+  }
+
+  function openBooking(offer) {
+    state.selectedOffer = offer;
+
+    const details = $("#bookingDetails");
+    const chosen = $("#chosenSummary");
+    if (chosen) {
+      chosen.innerHTML = "";
+
+      const t = document.createElement("div");
+      t.className = "chosen-title";
+      t.textContent = offer.unit?.name || offer.name || `Apartment ${offer.apartmentId}`;
+      chosen.appendChild(t);
+
+      const s1 = document.createElement("div");
+      s1.className = "chosen-sub";
+      s1.textContent = `${offer.arrival} → ${offer.departure}` + (offer.nights ? ` · ${offer.nights} Nächte` : "");
+      chosen.appendChild(s1);
+
+      const s2 = document.createElement("div");
+      s2.className = "chosen-sub";
+      s2.textContent = `${fmtMoney(offer.price, offer.currency) || "Preis auf Anfrage"} · ${offer.guests} Gäste`;
+      chosen.appendChild(s2);
+    }
+
+    if (details) {
+      details.style.display = "block";
+      details.scrollIntoView({ behavior: "smooth", block: "start" });
+    }
+    setBookStatus("");
+  }
+
+  function closeBooking() {
+    state.selectedOffer = null;
+    const details = $("#bookingDetails");
+    if (details) details.style.display = "none";
+    setBookStatus("");
+  }
+
+  function errToMessage(err) {
+    const d = err?.data;
+    if (typeof d === "string" && d.trim()) return d.trim();
+    if (d && typeof d === "object") {
+      return d.hint || d.error || d.message || (d.details && (d.details.hint || d.details.message)) || "";
+    }
+    return err?.message || "Fehler";
+  }
+
+  async function checkAvailability() {
+    const arrival = ($("#arrival")?.value || "").trim();
+    const departure = ($("#departure")?.value || "").trim();
+
+    const adults = Number(($("#adults")?.value || "").trim());
+    const children = Number(($("#children")?.value || "").trim());
+    const guests = (Number.isFinite(adults) ? adults : 0) + (Number.isFinite(children) ? children : 0);
+
+    if (!arrival || !departure) {
+      setStatus("Bitte Anreise und Abreise auswählen.", "error");
+      return;
+    }
+    const nights = dayDiff(arrival, departure);
+    if (!Number.isFinite(nights) || nights <= 0) {
+      setStatus("Bitte ein gültiges Datum wählen (Abreise muss nach Anreise liegen).", "error");
+      return;
+    }
+    if (!Number.isFinite(guests) || guests <= 0) {
+      setStatus("Bitte die Personenzahl angeben (mind. 1).", "error");
+      return;
+    }
+
+    closeBooking();
+    setStatus("Verfügbarkeit wird geprüft …");
+
+    const payload = {
+      arrivalDate: arrival,
+      departureDate: departure,
+      guests,
+    };
+    if (state.preselectApartmentId) payload.apartments = [state.preselectApartmentId];
+
+    try {
